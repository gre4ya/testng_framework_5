<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression">
    <test name="All Functionalities">
        <classes>

            <class name="scripts.TechGlobalDynamicElementsTest">
                <methods>
                    <include name="dynamicElementsTest"/>
                </methods>
            </class>

            <class name="scripts.TechGlobalFrontendTestingHomeTest">
                <methods>
                    <exclude name="validateFrontendTestingCards"/>
                    <include name="validateFrontendTestingURL"/>
                </methods>
            </class>

            <class name="scripts.TechGlobalHeaderTest">
                <methods>
                    <exclude name="validateTechGlobalHeaderDropdown"/>
                    <include name="validateTechGlobalHeaderDropdownOptions"/>
                </methods>
            </class>

            <class name="scripts.TechGlobalJavaExercisesHomeTest">
                <methods>
                    <include name="validateJavaExercisesCards"/>
                </methods>
            </class>

            <class name="scripts.TechGlobalRadioButtonsTest">
                <methods>
                    <include name="languageRadioButtons1"/>
                    <include name="javaRadioButtonValidation"/>
                    <include name="languageRadioButtons2"/>
                </methods>
            </class>

            <class name="scripts.TechGlobalCheckBoxTest"/>
            <class name="scripts.TechGlobalFooterTest"/>
            <class name="scripts.TechGlobalDropdownsTest"/>
            <class name="scripts.TechGlobalMultipleWindowsTest"/>
            <class name="scripts.TechGlobalAlertsTest"/>
            <class name="scripts.TechGlobalIFramesTest"/>
            <class name="scripts.TechGlobalStaticTablesTest"/>
            <class name="scripts.TechGlobalFileUploadTest"/>
            <class name="scripts.TechGlobalFileDownloadTest"/>
            <class name="scripts.TechGlobalSortableTablesTest"/>
            <class name="scripts.TechGlobalCalendarTest"/>
        </classes>
    </test>
</suite>